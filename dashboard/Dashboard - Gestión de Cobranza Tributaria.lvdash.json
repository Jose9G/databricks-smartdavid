{
  "datasets": [
    {
      "name": "50256db2",
      "displayName": "Performance Summary of Collection Agents Dataset",
      "queryLines": [
        "SELECT \r\n",
        "    colaborador,\r\n",
        "    fuente_cartera,\r\n",
        "    total_pagos_gestionados,\r\n",
        "    contribuyentes_unicos,\r\n",
        "    monto_total_recaudado\r\n",
        "FROM smartdavid_catalog.golden.rendimiento_colaborador\r\n",
        "ORDER BY monto_total_recaudado DESC;"
      ]
    },
    {
      "name": "caf31990",
      "displayName": "Cartera Pendiente por Departamento",
      "queryLines": [
        "SELECT \r\n",
        "    departamento,\r\n",
        "    COUNT(*) AS contribuyentes_pendientes,\r\n",
        "    ROUND(SUM(saldo), 2) AS saldo_por_recuperar\r\n",
        "FROM smartdavid_catalog.golden.cartera_pendiente\r\n",
        "WHERE departamento IS NOT NULL\r\n",
        "GROUP BY departamento\r\n",
        "ORDER BY saldo_por_recuperar DESC;"
      ]
    },
    {
      "name": "4d32dc82",
      "displayName": "Pagos por Mes",
      "queryLines": [
        "SELECT \r\n",
        "    mes_nombre,\r\n",
        "    mes,\r\n",
        "    anio,\r\n",
        "    fuente_cartera,\r\n",
        "    cantidad_pagos,\r\n",
        "    monto_total,\r\n",
        "    pagos_gestionados\r\n",
        "FROM smartdavid_catalog.golden.pagos_mensual\r\n",
        "ORDER BY anio, mes;"
      ]
    },
    {
      "name": "3933c2c6",
      "displayName": "KPIs Resumen",
      "queryLines": [
        "SELECT\r\n",
        "    COUNT(*)                          AS total_contribuyentes_pendientes,\r\n",
        "    ROUND(SUM(saldo), 2)              AS saldo_total_por_recuperar\r\n",
        "FROM smartdavid_catalog.golden.cartera_pendiente;"
      ]
    },
    {
      "name": "8cf1fac9",
      "displayName": "Distribuci√≥n PRICO vs MEPECO",
      "queryLines": [
        "SELECT \r\n",
        "    fuente_cartera,\r\n",
        "    SUM(total_pagos_gestionados) AS pagos_gestionados,\r\n",
        "    SUM(monto_total_recaudado)   AS monto_recaudado\r\n",
        "FROM smartdavid_catalog.golden.rendimiento_colaborador\r\n",
        "GROUP BY fuente_cartera;"
      ]
    }
  ],
